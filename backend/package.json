{
  "name": "aarogya-mitra-backend",
  "version": "1.0.0",
  "description": "Aarogya-Mitra Healthcare Management Backend",
  "main": "dist/index-postgres.js",
  "scripts": {
    "start": "node dist/index-postgres.js",
    "dev": "ts-node -r reflect-metadata src/index-postgres.ts",
    "dev:postgres": "ts-node -r reflect-metadata src/index-postgres.ts",
    "dev:multi-tenant": "ts-node -r reflect-metadata src/index-multi-tenant.ts",
    "dev:integrated": "ts-node -r reflect-metadata src/index-integrated.ts",
    "start:integrated": "node dist/index-integrated.js",
    "build": "tsc",
    "build:integrated": "tsc --project tsconfig.integrated.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "setup-aarogya": "ts-node src/scripts/runAarogyaSetup.ts",
    "setup-hierarchical": "ts-node src/scripts/setupAarogyaDBHierarchical.ts",
    "verify-hierarchical": "ts-node src/scripts/verifyHierarchicalDB.ts",
    "migrate:clinic-addresses": "ts-node src/scripts/migrateClinicAddresses.ts",
    "migrate:doctors": "ts-node src/scripts/migrate-doctors-receptionists.ts",
    "db:init": "ts-node -r reflect-metadata src/scripts/initializePostgres.ts",
    "db:init:force": "ts-node -r reflect-metadata src/scripts/initializePostgres.ts --force",
    "db:migrate:mongo-to-pg": "ts-node -r reflect-metadata src/scripts/migrateMongoToPostgres.ts",
    "db:init:shared": "ts-node -r reflect-metadata src/scripts/initializeShared.ts",
    "db:init:shared:force": "ts-node -r reflect-metadata src/scripts/initializeShared.ts --force",
    "db:setup:integrated": "ts-node -r reflect-metadata src/scripts/setupIntegratedDB.ts",
    "db:sample:past-visits": "ts-node -r reflect-metadata src/scripts/addSamplePastVisitData.ts"
  },
  "keywords": [
    "healthcare",
    "management",
    "aarogya-mitra",
    "medical"
  ],
  "author": "99Platforms",
  "license": "MIT",
  "dependencies": {
    "@99platforms/auth-sdk": "file:../../platforms_99/shared-auth-sdk",
    "@google/generative-ai": "^0.24.1",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "whatsapp-web.js": "^1.34.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.15.5",
    "jest": "^29.7.0",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
